class Compute 
{
    static pair getMinMax(long a[], long n)  
    {
        long max=a[0];
        long min=a[0];
        for (int i=0;i<n;i++)
        {
            max=(long)Math.max(max,a[i]);
            min=(long)Math.min(min,a[i]);
        }
        pair p=new pair(min,max);
        return p;
    }
}
